<keycloak-saml-adapter>
	<SP entityID="http://eap73.exmaple.com:8080/app-profile-saml/"
        sslPolicy="EXTERNAL"
        logoutPage="/index.jsp">
        <Keys>
            <Key signing="true">
                <PrivateKeyPem>
                    
                </PrivateKeyPem>
                <CertificatePem>
                    
                </CertificatePem>
            </Key>
        </Keys>
        <IDP entityID="idp"
             signatureAlgorithm="RSA_SHA256"
             signatureCanonicalizationMethod="http://www.w3.org/2001/10/xml-exc-c14n#">
            <SingleSignOnService signRequest="true"
                                 validateResponseSignature="true"
                                 validateAssertionSignature="false"
                                 requestBinding="POST"
                                 bindingUrl="http://rhsso.example.com:8080/auth/realms/master/protocol/saml"/>
            <SingleLogoutService signRequest="true"
                                 signResponse="true"
                                 validateRequestSignature="true"
                                 validateResponseSignature="true"
                                 requestBinding="POST"
                                 responseBinding="POST"
                                 postBindingUrl="http://rhsso.example.com:8080/auth/realms/master/protocol/saml"
                                 redirectBindingUrl="http://rhsso.example.com:8080/auth/realms/master/protocol/saml"/>
        </IDP>
    </SP>
</keycloak-saml-adapter>
